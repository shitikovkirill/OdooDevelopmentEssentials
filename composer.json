{
    "name": "amostajo/wordpress-plugin",
    "description": "A Wordpress Plugin Template that comes with composer and a lightweight MVC framework for Wordpress.",
    "license": "MIT",
    "keywords": ["wordpress","plugin","plugins","mvc"],
    "authors": [
        {
            "name": "Alejandro Mostajo",
            "email": "amostajo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "amostajo/wordpress-plugin-core": "1.0.*",
        "webdevstudios/cmb2": "dev-master"
    },
    "autoload": {
        "files": ["vendor/cmb2/init.php"],
        "psr-4": {
            "FullCalendar\\": "plugin"
        }
    },
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": ["webdevstudios/cmb2"]
        }
    },
    "minimum-stability": "dev"
}
